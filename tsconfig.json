{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["vite.config.*","env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "types": ["node"],
    "incremental": false,
    "composite": false,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
}
